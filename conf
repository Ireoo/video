if ( $host != "zhubo.pro") {
    rewrite ^/(.*)$ http://zhubo.pro/$1 permanent;
}
rewrite "/([0-9]+)$" /room.php?id=$1 last;
rewrite "/i$" /i.php last;
rewrite "/login$" /login.php last;
rewrite "/reg$" /reg.php last;

rewrite "/beta$" /beta.php last;
rewrite "/beta!([0-9]+)$" /beta.php?id=$1 last;

rewrite "/([0-9]+)/play$" /app/video/play.php?id=$1 last;
